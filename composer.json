	"type": "library",
	"description": "A simple Zend Framework 2 Blogging Module.",
	"homepage": "https://github.com/maglnet/MaglBlog",
	"license": "BSD-3",
	"keywords": ["blog", "blogging", "zend framework 2", "zf2"],
	"authors": [
		{
			"name": "Matthias Glaub",
			"email": "magl@magl.net",
			"role": "Developer"
		}
	],
	"require": {
		"php": ">=5.3.0",
		"zendframework/zendframework": "~2.2",
		"doctrine/doctrine-orm-module": "~0.8",
		"maglnet/magl-markdown" : "~1.0",
		"zf-commons/zfc-admin": "*"
	},
	"require-dev": {
		"phpunit/phpunit": "~4.0"
	},
	"autoload": {
		"classmap": ["src"],
		"psr-0": {"MaglBlog": "src"}
